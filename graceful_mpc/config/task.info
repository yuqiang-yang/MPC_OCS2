{
  runtimeMaxNumIterations        4
  initMaxNumIterations           50

  runtimeMinLearningRate	     1e-4
  runtimeMaxLearningRate         1.0
  initMinLearningRate            1e-4
  initMaxLearningRate            1.0


  debugPrint                     0
  coldStart                      0
